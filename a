
// vòng lặp for cho array viết theo cách thường
const arr = [0, 1, 2, 3, 4];
const arr2 = [];
for (let i = 0; i <arr.length; i++) {
    arr2.push(arr[i] ** 2);
}
console.log(arr2);

// viết ngắn hơn
const arr = [0, 1, 2, 3, 4];
const arr3 = arr.map((item) => item ** 2);
console.log(arr3);
// thêm ví dụ khác
const arr = [0, 1, 2, 3, 4];
const arr4 = arr.forEach((item) => console.log(item * 3));
